//-----------------------------------------------------------
//	SampleSkin.ClassicUT
//
//	Example class to demonstrate how to create a customized skin
//	for webadmin using unrealscript.
//-----------------------------------------------------------
class ClassicUT extends WebSkin;
// Name of class will be used for name of SkinPath

defaultproperties
{
     SubPath="ClassicUT"
     DisplayName="Classic UT"
     SkinCSS="classicut.css"
     DefaultBGColor="#000000"
}
